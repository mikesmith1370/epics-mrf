# Distributed bus bit @DBUS_BIT_NUM@ pulse generator trigger register (bit for pulse generator @PULSE_GEN_NUM@).

record(bo, "$(P)$(R)DBus:B@DBUS_BIT_NUM@:MapTo:TrigPulseGen@PULSE_GEN_NUM@") {
  field(DESC, "Map DBus bit @DBUS_BIT_NUM@ to pulse gen. @PULSE_GEN_NUM@")
  field(DTYP, "MRF Memory")
  field(OUT,  "@$(DEVICE) @DBUS_BIT_TO_PULSE_GEN_ADDR@[@PULSE_GEN_NUM@] uint32")
  field(ZNAM, "Disabled")
  field(ONAM, "Enabled")
}

