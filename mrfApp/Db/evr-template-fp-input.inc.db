# Front panel input @FP_INPUT_NUM@ mapping register.

record(bo, "$(P)$(R)FPIn@FP_INPUT_NUM@:LevelSensitivity") {
  field(DESC, "FP input @FP_INPUT_NUM@ level sensitivity")
  field(DTYP, "MRF Memory")
  field(OUT,  "@$(DEVICE) @FP_INPUT_ADDR@[29] uint32")
  field(ZNAM, "Active high")
  field(ONAM, "Active low")
}

